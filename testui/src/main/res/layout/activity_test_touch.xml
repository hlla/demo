<example.com.testui.TestFrameLaout xmlns:android="http://schemas.android.com/apk/res/android"
                                   xmlns:tools="http://schemas.android.com/tools"
                                   android:id="@+id/test_root"
                                   android:layout_width="match_parent"
                                   android:layout_height="match_parent"
                                   android:layout_gravity="center"
                                   android:orientation="horizontal"
    >

    <ImageView
        android:id="@+id/img0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="300dip"
        android:background="#0000ff"
        android:src="@drawable/eye_guide_bg"
        android:visibility="gone"/>

    <example.com.testui.TestLinearLayout
        android:id="@+id/test_content"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        >

        <example.com.testui.TestTextView
            android:id="@+id/text_id"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="top"
            android:layout_marginLeft="40dip"
            android:background="#00ff00"
            android:clickable="true"
            android:gravity="center"
            android:text="fdsfsdfdf"
            />

        <Button
            android:id="@+id/test_notify"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/eye_guide_bg"
            android:text="@string/test_notify"
            />

        <Button
            android:id="@+id/cancel_notify"
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:background="#ff0000"
            android:text="@string/cancel_notify"
            />
    </example.com.testui.TestLinearLayout>
    <!--<Button-->
    <!--android:id="@+id/hide_cv"-->
    <!--android:layout_width="wrap_content"-->
    <!--android:layout_height="wrap_content"-->
    <!--android:text="@string/hide_cv"/>-->

    <!--<Button-->
    <!--android:id="@+id/hide_pv"-->
    <!--android:layout_width="wrap_content"-->
